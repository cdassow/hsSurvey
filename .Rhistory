dlogistic <- function(K,rd,N0,t) {
N = c(N0, numeric(t))
for(i in 1:t) {
N[i+1] = N[i] + rd * N[i] * (1 - (1/K[i]) * N[i])
}
return(N)
}
d = dlogistic(K, rd, 1, 20)
plot(0:20,d)
plot(ylab="growth", xlab="years",0:20,d)
